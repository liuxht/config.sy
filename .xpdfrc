bind	G	any	gotoLastPage
bind	=	any	zoomIn
bind	d	any	pageDown
bind	u	any	pageUp
bind	c	any	toggleContinuousMode
bind	t	any	toggleOutline
bind	f	any	toggleFullScreenMode
